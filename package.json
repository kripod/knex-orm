{
  "name": "knex-orm",
  "version": "1.2.0",
  "description": "Knex-based object-relational mapping for JavaScript.",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "orm",
    "knex"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "start": "babel-node ./example",
    "build": "babel ./src -d ./lib -s",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build",
    "init-db": "npm run babel-knex -- migrate:latest && npm run babel-knex -- seed:run",
    "test": "cross-env NODE_ENV=test nyc mocha --require babel-core/register --recursive -b",
    "doc": "documentation build ./src/index.js -o ./docs -f html",
    "babel-knex": "babel-node ./node_modules/knex/lib/bin/cli"
  },
  "dependencies": {
    "ajv": "^4.0.5",
    "babel-runtime": "^6.6.1",
    "es6-error": "^3.0.0",
    "inflection": "^1.10.0"
  },
  "peerDependencies": {
    "knex": ">=0.6.10 <0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "istanbul": "^0.4.3",
    "knex": "^0.11.10",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "sqlite3": "^3.1.4",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kripod/knex-orm.git"
  },
  "bugs": {
    "url": "https://github.com/kripod/knex-orm/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
